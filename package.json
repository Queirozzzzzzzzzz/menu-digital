{
  "name": "menu-digital",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "rawdev": "next dev",
    "dev": "npm run services:up && npm run migration:up && next dev",
    "build": "next build",
    "create:admin": "curl -X POST http://localhost:3000/api/v1/users/admin",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "test": "clear && jest --runInBand",
    "test:silent": "clear && jest --runInBand --silent",
    "test:watch": "jest --watchAll --runInBand",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up"
  },
  "author": "Out of Box",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "apexcharts": "^4.1.0",
    "async-retry": "1.3.3",
    "bcryptjs": "2.4.3",
    "cookie": "0.6.0",
    "joi": "17.13.3",
    "next": "14.2.4",
    "next-connect": "0.13.0",
    "node-pg-migrate": "7.5.2",
    "pg": "8.12.0",
    "pino": "9.3.1",
    "react": "18.3.1",
    "react-apexcharts": "^1.6.0",
    "set-cookie-parser": "^2.6.0",
    "snakeize": "^0.1.0",
    "sonner": "1.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "uuid": "^10.0.0"
  }
}
